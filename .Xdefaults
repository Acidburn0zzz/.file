Xft.dpi:                96
Xft.antialias:          true
Xft.hinting:            true
Xft.rgba:               rgb
Xft.autohint:           true
Xft.hintstyle:          hintfull
Xft.lcdfilter:          lcddefault

urxvt*font:             xft:profont:pixelsize=11,xft:MS Gothic,xft:Bitstream Vera Sans
